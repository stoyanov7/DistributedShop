{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "Database": "distributedShop-identityDb",
    "Seed": false
  },
  "JwtSettings": {
    "secretKey": "MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY",
    "expiryMinutes": 30,
    "issuer": "distributedShop-identity",
    "validateLifetime": true
  },
  "SwaggerSettings": {
    "enabled": true,
    "name": "v1",
    "title": "Identity Layer",
    "version": "v1",
    "routePrefix": "docs"
  }
}
